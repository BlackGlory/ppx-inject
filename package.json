{
  "name": "ppx-inject",
  "version": "0.2.2",
  "bin": "lib/cli.js",
  "files": [
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/ppx-inject.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint src/** __tests__/** --ext .js,.jsx,.ts,.tsx",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "coverage": "jest --coverage --config jest.config.js",
    "dev": "ts-node src/index.ts",
    "dev:debug": "node --inspect-brk node_modules/.bin/ts-node src/index.ts",
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rm -rf lib",
    "build": "tsc --project tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./lib"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test"
    }
  },
  "dependencies": {
    "fs-extra": "^9.0.0",
    "immer": "^6.0.5",
    "internet-number": "^2.2.1",
    "ip": "^1.1.5",
    "ip-address": "^6.3.0",
    "iterable-operator": "^0.10.1",
    "nanoid": "^3.1.7",
    "tmp-promise": "^3.0.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.1.0",
    "@types/ip": "^1.1.0",
    "@types/ip-address": "^5.8.2",
    "@types/jest": "^25.2.1",
    "@types/node": "^14.0.1",
    "@types/tmp": "^0.2.0",
    "@types/xml2js": "^0.4.5",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.5.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "ts-jest": "^25.5.1",
    "ts-node": "^8.10.1",
    "tscpaths": "^0.0.9",
    "typescript": "^3.9.2"
  }
}
