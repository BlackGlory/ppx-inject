{
  "name": "ppx-inject",
  "version": "0.2.3",
  "bin": "lib/cli.js",
  "files": [
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/ppx-inject.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "coverage": "jest --coverage --config jest.config.js",
    "dev": "ts-node src/index.ts",
    "dev:debug": "node --inspect-brk node_modules/.bin/ts-node src/index.ts",
    "prepublishOnly": "yarn clean && yarn build",
    "clean": "rm -rf lib",
    "build": "tsc --project tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "dependencies": {
    "@blackglory/structures": "^0.1.1",
    "fs-extra": "^9.0.1",
    "immer": "^8.0.0",
    "internet-number": "^2.2.2",
    "ip": "^1.1.5",
    "ip-address": "^7.1.0",
    "iterable-operator": "^0.12.2",
    "nanoid": "^3.1.20",
    "tmp-promise": "^3.0.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/fs-extra": "^9.0.5",
    "@types/ip": "^1.1.0",
    "@types/ip-address": "^7.0.0",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/tmp": "^0.2.0",
    "@types/xml2js": "^0.4.7",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "eslint": "^7.16.0",
    "husky": "^4.3.6",
    "jest": "^26.6.3",
    "standard-version": "^9.0.0",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "tscpaths": "^0.0.9",
    "typescript": "^4.1.3"
  }
}
